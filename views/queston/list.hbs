<h3><a class="btn btn-secondary" href="/"><i class="fa fa-plus"></i> Create New</a> Question & Answer List</h3>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Question</th>
            <th></th>
            <th>Answer</th>



        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fullName}}</td>
            <td>|</td>
            <td>{{this.shortName}}</td>

            <td>
                <a href="/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/delete/{{this._id}}"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
